{
  "apiVersion": "core.meshery.io/v1alpha1",
  "kind": "Edge",
  "metadata": {
    "description": "A relationship that defines network edges between components"
  },
  "model":{
    "name":"core"
  },
  "subType": "Network",
  "selectors": {
    "allow": {
      "from": [
        {
          "kind": "Service",
          "model": "kubernetes"
        }
      ],
      "to": [
        {
          "kind": "Endpoints",
          "model": "kubernetes"
        },
        {
          "kind": "Pod",
          "model": "kubernetes"
        },
        {
          "kind": "Deployment",
          "model": "kubernetes"
        }
      ]
    },
    "deny": {
      "from": [],
      "deny": []
    }
  }
}
